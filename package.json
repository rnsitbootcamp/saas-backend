{
  "name": "retail-saas",
  "version": "1.0.178",
  "description": "Backend server for Retail SaaS",
  "main": "dist/Server.js",
  "scripts": {
    "test": "npm run lint-test && npm run depcheck && echo \"coming soon ... more tests....\"",
    "lint-test": "tslint src/**/*.ts -c ./tslint.json",
    "depcheck": "depcheck ./  --ignores=\"mocha,eslint,eslint-*,babel-*,@types/*,chai\"",
    "debug": "PORT=3030 nodemon --inspect dist/Server.js",
    "deploy": "npm ci && rm -rf dist/ && npm run build && npm run docs && npm start",
    "start": "pm2 startOrRestart ecosystem.config.js --env=$NODE_ENV --update-env",
    "dev": "gulp",
    "build": "gulp ts",
    "docs": "apidoc -i src/ -o docs/",
    "tasks": "nodemon dist/tasks",
    "twatch": "tsc -w",
    "tbuild": "tsc"
  },
  "author": "",
  "license": "ISC",
